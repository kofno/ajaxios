{
  "name": "ajaxios",
  "version": "3.1.0",
  "description": "Http requests with better error handling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Ryan L. Bell <ryan.l.bell@gmail.com>",
  "license": "MIT",
  "scripts": {
    "docs": "typedoc src/index.ts --out docs --theme minimal && touch docs/.nojekyll",
    "test": "bun test",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "format": "bun format --write ."
  },
  "dependencies": {
    "axios": "^1.8.1",
    "jsonous": "10.0.0-pre.0",
    "maybeasy": "^6.3.0",
    "resulty": "^8.0.0",
    "taskarian": "^6.1.0"
  },
  "devDependencies": {
    "typedoc": "^0.20.36",
    "typescript": "^5.8.2",
    "tsup": "^8.0.2",
    "prettier": "^3.0.0",
    "@types/bun": "^1.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kofno/ajaxios.git"
  },
  "module": "dist/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 80
  }
}
